"\e/": nop  # complete-filename
"\e@": nop  # complete-hostname
"\e{": nop  # complete-into-braces
"\e~": nop  # complete-username
"\e$": nop  # complete-variable
"\C-d": nop  # delete-char
"\e[3~": nop  # delete-char
"\e\\": nop  # delete-horizontal-space
"\e-": nop  # digit-argument
