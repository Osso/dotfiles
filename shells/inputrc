# Unbind

"\C-g": nop  # abort
"\C-x\C-g": nop  # abort
"\e\C-g": nop  # abort
"\C-j": nop  # accept-line
"\C-b": nop  # backward-char
"\eOD": nop  # backward-char
"\e[D": nop  # backward-char
"\C-?": nop  # backward-delete-char
"\C-x\C-?": nop  # backward-kill-line
"\e\C-h": nop  # backward-kill-word
