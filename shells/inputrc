# Bind

"\C-m": accept-line
"\C-_": undo
"\C-k": kill-line
"\C-r": reverse-search-history
"\C-o": operate-and-get-next
"\C-l": clear-screen
# Ctrl+<arrow>
"\e[1;5C": forward-word
"\e[1;5D": backward-word
# Alt+<arrow>
"\e[1;3D": beginning-of-line
"\e[1;3C": end-of-line
# Ctrl+delete
"\e[3;5~": kill-word
# Ctrl+backspace
"\C-H": backward-kill-word
# Alt+backspace
"\e\C-?" backward-kill-line
# Alt+delete
"\e[3;3~": kill-line
